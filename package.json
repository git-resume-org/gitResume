{
  "name": "git-resume",
  "version": "0.0.1",
  "description": "Generate bullet points for your resume from your git commit history",
  "exports": "./index.js",
  "type": "module",
  "node": ">=18",
  "scripts": {
    "start": "webpack serve --config webpack.config.mjs --hot --open --mode development",
    "build": "webpack --config webpack.config.mjs --mode production",
    "dev": "concurrently --kill-others-on-fail \"npm run start\" \"nodemon server/server.js\"",
    "lint": "eslint . && stylelint 'client/**/*.scss'",
    "lint:fix": "eslint . --fix && stylelint 'client/**/*.scss' --fix",
    "prod": "npm run build && npm run serve",
    "prod-debug": "npm run build && npm run serve-debug",
    "serve": "NODE_ENV=production node server/server.js",
    "serve-debug": "DEBUG=* npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/git-resume-org/gitResume.git"
  },
  "keywords": [
    "resume",
    "ai",
    "commits",
    "commit",
    "history",
    "git"
  ],
  "author": "git-resume-org",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/git-resume-org/gitResume/issues"
  },
  "homepage": "https://github.com/git-resume-org/gitResume#readme",
