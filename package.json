{
  "name": "git-resume",
  "version": "0.0.1",
  "description": "Generate bullet points for your resume from your git commit history",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "concurrently --kill-others-on-fail \"npm run serve\" \"nodemon server/server.js\"",
    "lint": "eslint . && stylelint 'client/**/*.scss'",
    "lint:fix": "eslint . --fix && stylelint 'client/**/*.scss' --fix",
    "serve": "webpack serve --open --hot --mode development"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pikachublessyou/gitResume.git"
  },
  "keywords": [
    "resume",
    "ai",
    "commits",
    "commit",
    "history",
    "git"
  ],
  "author": "pikachublessyou",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pikachublessyou/gitResume/issues"
  },
  "homepage": "https://github.com/pikachublessyou/gitResume#readme",
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-class-properties": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-react": "^7.24.6",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "dot-env": "^0.0.1",
    "eslint": "^9.3.0",
    "highlight.js": "^11.9.0",
    "html-webpack-plugin": "^5.6.0",
    "modern-normalize": "^2.0.0",
    "nodemon": "^3.1.2",
    "normalize.css": "^8.0.1",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.1",
    "postcss-scss": "^4.0.9",
    "sass": "^1.77.2",
    "sass-loader": "^14.2.1",
    "style-loader": "^4.0.0",
    "stylelint": "^16.6.1",
    "stylelint-config-recommended": "^14.0.0",
